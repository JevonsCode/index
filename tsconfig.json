{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@service/*": ["src/service/*"],
      "@store": ["src/store"],
      "@store/*": ["src/store/*"],
      "@database/*": ["src/database/*"]
    },
    "typeRoots": ["src/store/indexs/types.d.ts", "./node_modules/@types"]
  },
  "include": ["src"],
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS",
      "strict": false
    }
  }
}
